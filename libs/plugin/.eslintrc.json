{
	"extends": ["../../.eslintrc.json"],
	"ignorePatterns": ["!**/*"],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.json"],
			"parser": "jsonc-eslint-parser",
			"rules": {
				"@nx/dependency-checks": [
					"warning",
					{
						"ignoredDependencies": [
							"tslib",
							"ts-morph",
							"@angular-eslint/bundled-angular-compiler",
							"@nx/devkit",
							"@nx/dependency-checks"
						]
					}
				]
			}
		},
		{
			"files": ["./package.json", "./generators.json"],
			"parser": "jsonc-eslint-parser",
			"rules": {
				"@nx/nx-plugin-checks": "warning"
			}
		}
	]
}
